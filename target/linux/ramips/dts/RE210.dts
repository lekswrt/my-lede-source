/dts-v1/;

#include "mt7620a.dtsi"
#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	compatible = "ralink,mt7620a-soc";
	model = "TP-LINK RE210 v1";

	gpio-leds {
		compatible = "gpio-leds";

		wlan5g {
			label = "re210:green:wlan5g";
			gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
		};

		wlan2g {
			label = "re210:green:wlan2g";
			gpios = <&gpio3 0 GPIO_ACTIVE_LOW>;
		};

		power {
			label = "re210:green:power";
			gpios = <&gpio1 3 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};

		range_g {
			label = "re210:green:range_g";
			gpios = <&gpio1 1 GPIO_ACTIVE_LOW>;
		};

		range_r {
			label = "re210:red:range_r";
			gpios = <&gpio1 2 GPIO_ACTIVE_LOW>;
		};
	};

	gpio-keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;

		reset {
			label = "reset";
			gpios = <&gpio0 13 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		led {
			label = "led";
			gpios = <&gpio1 0 GPIO_ACTIVE_LOW>;
			linux,code = <BTN_0>;
		};

		power {
			label = "power";
			gpios = <&gpio1 4 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_POWER>;
		};
	};
};

&gpio1 {
	status = "okay";
};

&gpio2 {
	status = "okay";
};

&gpio3 {
	status = "okay";
};

&spi0 {
	status = "okay";

	m25p80@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partition@0 {
			label = "u-boot";
			reg = <0x0 0x20000>;
			read-only;
		};

		partition@20000 {
			label = "firmware";
			reg = <0x20000 0x7e0000>;
		};

		config: partition@7e0000 {
			label = "stock_config";
			reg = <0x7e0000 0x10000>;
			read-only;
		};

		art: partition@7f0000 {
			label = "art";
			reg = <0x7f0000 0x10000>;
			read-only;
		};
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "i2c", "uartf", "rgmii1", "wled";
			ralink,function = "gpio";
		};

		pa {
			ralink,group = "pa";
			ralink,function = "pa";
		};
	};
};

&ethernet {
	mtd-mac-address = <&config 0x158>;
	pinctrl-names = "default";
	pinctrl-0 = <&rgmii2_pins &mdio_pins>;

	port@4 {
		status = "okay";
		phy-handle = <&phy4>;
		phy-mode = "rgmii";
	};

	mdio-bus {
		status = "okay";

		phy4: ethernet-phy@4 {
			reg = <4>;
			phy-mode = "rgmii";
			/*max-speed = <100>;*/
		};
	};
};

&gsw {
	mediatek,port4 = "gmac";
};

&wmac {
	ralink,mtd-eeprom = <&art 0>;
	mtd-mac-address = <&config 0x158>;
};


&pcie {
	status = "okay";

	pcie-bridge {
		mt76@0,0 {
			reg = <0x0000 0 0 0 0>;
			device_type = "pci";
			mediatek,mtd-eeprom = <&art 0x8000>;
			mtd-mac-address = <&config 0x158>;
			mtd-mac-address-increment = <2>;
		};
	};
};
